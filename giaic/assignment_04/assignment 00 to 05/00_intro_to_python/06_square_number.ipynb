     ]
    }
   ],
   "source": [
    "# Prompt the user to input a number\n",
    "number = float(input(\"Type a number to see its square: \"))\n",
    "\n",
    "# Calculate the square of the number\n",
    "square = number ** 2\n",
    "\n",
    "# Print the result\n",
    "print(f\"{number} squared is {square}\")"
   ]
